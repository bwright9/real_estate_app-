<%= form_for [@seller, @home] do |f| %>
  
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :square_feet %>
  <%= f.text_field :square_feet %>

  <%= f.label :beds %>
  <%= f.text_field :beds %>

  <%= f.label :baths %>
  <%= f.text_field :baths %>

  <%= f.label :home_type %>
  <%= f.text_field :home_type %>

  <%= f.label :price %>
  <%= f.text_field :price %>

  <%= f.label :active %>
  <%= f.select :active, [true, false] %>

  <%= f.submit %>
<% end %>